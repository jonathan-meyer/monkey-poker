{
  "name": "monkey-poker",
  "version": "2.0.0",
  "description": "Agile Planning Poker App",
  "main": "build/server",
  "scripts": {
    "start": "node .",
    "start-dev-client": "npm run build-dev-server && cross-env WEBPACK_MODE=development PORT=5000 DEBUG=@slack/events-api:* node dist/server",
    "start-dev-server": "npm run build-dev-server && concurrently npm:watch:build-dev-server npm:watch:start-dev-server",
    "build-dev-server": "rimraf dist && tsc --outDir dist",
    "watch:build-dev-server": "tsc --outDir dist --watch --preserveWatchOutput",
    "watch:start-dev-server": "cross-env PORT=5000 DEBUG=@slack/events-api:* nodemon dist/server",
    "test": "jest --coverage",
    "lint": "eslint src",
    "clean": "rimraf build build-dev dist coverage",
    "prebuild": "rimraf build",
    "build": "npm run build-client && npm run build-server",
    "build-client": "cross-env WEBPACK_MODE=production webpack -p --config=config/webpack.config.prod.js",
    "build-server": "tsc"
  },
  "author": "Jonathan Meyer",
  "license": "ISC",
  "engines": {
    "node": "12.x",
    "npm": "7.x"
  },
